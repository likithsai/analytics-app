{
  "name": "analytics-tracker",
  "version": "1.0.0",
  "description": "A lightweight analytics tracking library inspired by Google Analytics and Matomo, written in TypeScript.",
  "main": "dist/tracker.min.js",
  "module": "dist/tracker.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config webpack.config.js --mode production",
    "dev": "webpack --config webpack.config.js --mode development --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "analytics",
    "tracker",
    "typescript",
    "browser-tracking"
  ],
  "author": "Likith Sai",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ]
}
